{
  "name": "Lab Node Working with SPA Applications",
  "version":"1.0.0",
  "resources": "resources.yml",
  "postConfigureTour": ".auth0/lab/guides/main.tour",
  "clients": [
    {
      "name":"Lab: SPA",
      "directory": "src/spa-app",
      "env": {
        "API_URL": "Lab: API",
        "AUTH0_DOMAIN": "$tenant_domain",
        "CLIENT_ID": "$client_id",
        "PORT": 8000
      }
    }
  ],
  "resourceServers": [
    {
      "name":"Lab: API",
      "directory": "src/api",
      "env": {
        "ISSUER_BASE_URL": "$tenant_base_url",
        "ALLOWED_AUDIENCES": "$identifier",
        "PORT": 5000
      }
    }
  ]
}
